{
  "ts-node": {
    "esm": true
  },
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "sourceMap": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "declaration": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "webworker"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node",
      "jest"
    ]
  }
}
